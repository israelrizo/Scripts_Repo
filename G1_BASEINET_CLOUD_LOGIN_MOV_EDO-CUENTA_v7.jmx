            <stringProp name="script">import java.io.File;
import java.io.FileWriter;
import java.io.BufferedWriter;
import java.text.SimpleDateFormat;
import java.util.Date;
import org.apache.jmeter.services.FileServer;

// Helper para CSV: quita saltos de línea y escapa comillas
String csv(String s) {
    if (s == null) return &quot;&quot;;
    s = s.replaceAll(&quot;[\\r\\n]+&quot;, &quot; &quot;);
    s = s.replace(&quot;\&quot;&quot;, &quot;\&quot;\&quot;&quot;); // escape de comillas dobles
    return &quot;\&quot;&quot; + s + &quot;\&quot;&quot;;
}

// Fecha para carpeta si no la tienes definida
String formattedDate;
try {
    formattedDate = vars.get(&quot;formattedDate&quot;);
    if (formattedDate == null || formattedDate.trim().length() == 0) {
        formattedDate = new SimpleDateFormat(&quot;yyyyMMdd_HHmmss&quot;).format(new Date());
        vars.put(&quot;formattedDate&quot;, formattedDate);
    }
} catch (Throwable t) {
    formattedDate = new SimpleDateFormat(&quot;yyyyMMdd_HHmmss&quot;).format(new Date());
}

// Base del test plan
String baseDir = FileServer.getFileServer().getBaseDir();

// Carpeta y archivo
String relativePath = &quot;Results/MainTest/&quot; + formattedDate;
String fileName = &quot;Usuarios_fallados.csv&quot;;

File directory = new File(baseDir, relativePath);
if (!directory.exists()) {
    if (!directory.mkdirs()) {
        log.error(&quot;No se pudo crear el directorio: &quot; + directory.getAbsolutePath());
        throw new RuntimeException(&quot;No se pudo crear el directorio: &quot; + directory.getAbsolutePath());
    } else {
        log.info(&quot;Directorio creado: &quot; + directory.getAbsolutePath());
    }
}

File file = new File(directory, fileName);
boolean nuevoArchivo = !file.exists();

BufferedWriter buff = null;
FileWriter fWriter = null;

try {
    fWriter = new FileWriter(file, true); // append
    buff = new BufferedWriter(fWriter);

    // Escribe encabezado solo si es nuevo
    if (nuevoArchivo) {
        String header = &quot;P_Username,C_maskedUserName,Response Code,Response Message,RequestData&quot;;
        buff.write(header);
        buff.newLine();
    }

    // Solo guardamos si falló el sampler
    if (!prev.isSuccessful()) {
        String userId = vars.get(&quot;P_Username&quot;);
        String masked = vars.get(&quot;C_maskedUserName&quot;);
        String responseCode = prev.getResponseCode();
        String responseMessage = prev.getResponseMessage();
        String requestData = prev.getSamplerData();

        String line = csv(userId) + &quot;,&quot; +
                      csv(masked) + &quot;,&quot; +
                      csv(responseCode) + &quot;,&quot; +
                      csv(responseMessage) + &quot;,&quot; +
                      csv(requestData);

        buff.write(line);
        buff.newLine();
        log.info(&quot;Error registrado en: &quot; + file.getAbsolutePath());
    }
} catch (Exception e) {
    log.error(&quot;Error escribiendo archivo: &quot; + e.getMessage());
    throw e;
} finally {
    try { if (buff != null) buff.close(); } catch (Exception ex) {}
    try { if (fWriter != null) fWriter.close(); } catch (Exception ex) {}
}
