    // def responseDataTrimmed = responseData?.length() &gt; maxLen ? responseData.substring(0, maxLen) + &quot;...[truncated]&quot; : responseData
    def responseDataTrimmed = responseData

    synchronized (this.class) {
        file.parentFile.mkdirs()
