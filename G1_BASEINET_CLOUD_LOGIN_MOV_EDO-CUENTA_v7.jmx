              <stringProp name="script">// Define the file path and name for your CSV
def filePath = &quot;/Users/israellopezrizo/Downloads/jmeter_extractors/output_data.csv&quot; 
    
// Get the extracted value from the JMeter variable
//User Data 
def Usuario = vars.get(&quot;P_Username&quot;)
def Pass = vars.get(&quot;P_Password&quot;)
def Token = vars.get(&quot;P_Token&quot;)

def matchCountA = vars.get(&quot;C_Moneda_matchNr&quot;) as int
def matchCountD = vars.get(&quot;C_DestinatarioTipo_matchNr&quot;) as int

def header = &quot;Usuario,Password,Token,Moneda,Cuenta,Saldo,Activo,Tipo,Banco,Moneda SPEI,Clabe,Activo SPEI,Persona,RFC,Email,Nombre Completo,Activado&quot;
    
def csvFile = new File(filePath)

// Write a header row only if the file doesn&apos;t exist
if (!csvFile.exists()) {
    csvFile.write(header + System.getProperty(&quot;line.separator&quot;))
}

// Append the new data line to the file
synchronized (csvFile) {
	if(matchCountA&gt;0){
	for (int i = 1; i &lt;= matchCountA; i++) {
          //Account Details
		def Activo = vars.get(&quot;C_isActive_&quot; + i)
		def Moneda = vars.get(&quot;C_Moneda_&quot; + i)
		def NumCta = vars.get(&quot;C_Cuenta_&quot; + i)
		def Saldo = vars.get(&quot;C_Saldo_&quot; + i)

		if(!Moneda.equalsIgnoreCase(&quot;MXP&quot;)){
			log.info(&quot;Skipping Account $i: Moneda is not MXP.&quot;)
			continue
		}
		
		def baseAccount = Usuario + &quot;,&quot; + Pass + &quot;,&quot; + Token + &quot;,&quot; + Moneda + &quot;,&quot; + NumCta + &quot;,&quot; + Saldo + &quot;,&quot; + Activo

		if (matchCountD &gt; 0) {
			for (int j = 1; j &lt;= matchCountD; j++) {

				//Destinatarios Details -&gt; 
				def Tipo = vars.get(&quot;C_DestinatarioTipo_&quot; + j)
				def Banco = vars.get(&quot;C_DestinatarioBanco_&quot; + j)
				def Moneda_2 = vars.get(&quot;C_DestinatarioMoneda_&quot; + j)
				def Clabe = vars.get(&quot;C_DestinatarioClabe_&quot; + j)
				def ActivoD = vars.get(&quot;C_DestinatarioActivo_&quot; + j)
				def Persona = vars.get(&quot;C_DestinatarioPersona_&quot; + j)
				def RFC = vars.get(&quot;C_DestinatarioRFC_&quot; + j)
				def Email = vars.get(&quot;C_DestinatarioEmail_&quot; + j)
				def Nombre = vars.get(&quot;C_DestinatarioFullName_&quot; + j)
				def Activado = vars.get(&quot;C_DestinatarioActivado_&quot; + j)

				if(!Tipo.equalsIgnoreCase(&quot;SPEI&quot;) || !Moneda_2.equalsIgnoreCase(&quot;MXP&quot;)){
					log.info(&quot;Skipping Destinatario $j: Moneda is not MXN or is not an SPEI account&quot;)
					continue
				}

				def Destinatario = Tipo + &quot;,&quot; + Banco + &quot;,&quot; + Moneda_2 + &quot;,&quot; + Clabe + &quot;,&quot; + ActivoD + &quot;,&quot; + Persona + &quot;,&quot; + RFC + &quot;,&quot; + Email + &quot;,&quot; + Nombre + &quot;,&quot; + Activado

				def line = baseAccount + &quot;,&quot; + Destinatario
				
			     csvFile.append(line + System.getProperty(&quot;line.separator&quot;))
			     log.info(&quot;Line printed =&gt; $line&quot;)
			}
		}
     }
	}
}</stringProp>
