            <stringProp name="script">// Get the base directory where the JMeter test plan is located
String baseDir = FileServer.getFileServer().getBaseDir();

// Define the relative directory and file name
String relativePath = &quot;Resultados/MainTest/&quot; + formattedDate;
String fileName = &quot;Usuarios_fallados.csv&quot;;

// Construct the full directory path
File directory = new File(baseDir, relativePath);
if (!directory.exists()) {
    if (directory.mkdirs()) {
        log.info(&quot;Directory created: &quot; + directory.getAbsolutePath());
    } else {
        log.error(&quot;Failed to create directory: &quot; + directory.getAbsolutePath());
        throw new RuntimeException(&quot;Failed to create directory: &quot; + directory.getAbsolutePath());
    }
}

// Create the file writer and buffered writer
try {
    File file = new File(directory, fileName);

    // Check if the file exists; if not, write the header row
    boolean fileExists = file.exists();

    FileWriter fWriter = new FileWriter(file, true); // Append mode
    BufferedWriter buff = new BufferedWriter(fWriter);

    if (!fileExists) {
        // Write the header row
        buff.write(&quot;P_Username&quot;, &quot;C_maskedUserName&quot;,&quot;Response Code&quot;, &quot;Response Message&quot;, &quot;RequestData&quot;);
        buff.newLine();
    }

// Revisar si el request falló
if (!prev.isSuccessful()) {
    // Variables que quieres guardar
    def userId = vars.get(&quot;P_Username&quot;)     // ejemplo: variable del CSV
    def responseCode = prev.getResponseCode()
    def responseMessage = prev.getResponseMessage()
    def requestData = prev.getSamplerData()

    // Línea a guardar (CSV)
    def line = &quot;${userId},${responseCode},${responseMessage},${requestData}\n&quot;

    buff.write(line);
    buff.newLine();

    // Close writers
    buff.close();
    fWriter.close();

}

    log.info(&quot;Data written successfully to &quot; + file.getAbsolutePath());
} catch (Exception e) {
    log.error(&quot;Error writing to file: &quot; + e.getMessage());
    throw e; // Rethrow to let JMeter handle the error
}

