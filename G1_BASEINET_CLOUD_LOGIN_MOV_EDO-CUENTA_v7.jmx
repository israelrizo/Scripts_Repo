def expr = &quot;normalize-space(translate(/s:Envelope/s:Body/t:ScValidateCuentaResponse/t:ScValidateCuentaResult, &apos;*&apos;, &apos;&apos;))&quot;
def gotNoStars = xp.evaluate(expr, doc)

