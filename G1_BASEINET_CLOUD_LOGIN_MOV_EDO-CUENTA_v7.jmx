// Check if the sampler supports getUrl()
if (sampler instanceof org.apache.jmeter.protocol.http.sampler.HTTPSamplerBase) {
    apiUrl = sampler.getUrl().toString();
} else {
    log.warn(&quot;Sampler &apos;&quot; + sampler.getName() + &quot;&apos; does not support getUrl()&quot;);
}

// Combine values like in signBodyRequest()
String rawData = apiUrl + navigationData;

