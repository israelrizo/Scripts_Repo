            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import javax.xml.parsers.DocumentBuilderFactory
import javax.xml.xpath.XPathFactory
import javax.xml.namespace.NamespaceContext
import org.xml.sax.InputSource
import org.apache.jmeter.services.FileServer
import java.text.SimpleDateFormat

// ===== Helpers =====
String csv(String s) {
    if (s == null) return &quot;&quot;
    s = s.replaceAll(/[\\r\\n]+/, &quot; &quot;).replace(&quot;\&quot;&quot;,&quot;\&quot;\&quot;&quot;)
    return &quot;\&quot;${s}\&quot;&quot;
}
String nowStr() {
    return new SimpleDateFormat(&quot;yyyy-MM-dd HH:mm:ss&quot;).format(new Date())
}
String ensureFormattedDate() {
    def fd = vars.get(&quot;formattedDate&quot;)
    if (!fd || fd.trim().isEmpty()) {
        fd = new SimpleDateFormat(&quot;yyyyMMdd&quot;).format(new Date())
        vars.put(&quot;formattedDate&quot;, fd)
    }
    return fd
}

// ===== Parseo y comparación =====
def expectedNoStars = (vars.get(&apos;C_maskedUserName&apos;) ?: &apos;&apos;).trim()
def responseData = prev.getResponseDataAsString()
def body = responseData

def dbf = DocumentBuilderFactory.newInstance()
dbf.setNamespaceAware(true)
def doc = dbf.newDocumentBuilder().parse(new InputSource(new StringReader(body)))

def xp = XPathFactory.newInstance().newXPath()
xp.setNamespaceContext(new NamespaceContext(){
  String getNamespaceURI(String prefix){
    if (prefix == &apos;s&apos;) return &apos;http://schemas.xmlsoap.org/soap/envelope/&apos;
    if (prefix == &apos;t&apos;) return &apos;http://tempuri.org/&apos;
    return javax.xml.XMLConstants.NULL_NS_URI
  }
  String getPrefix(String ns){ return null }
  java.util.Iterator getPrefixes(String ns){ return null }
})

def expr = &quot;normalize-space(translate(/s:Envelope/s:Body/t:ScValidateCuentaResponse/t:ScValidateCuentaResult, &apos;*&apos;, &apos;&apos;))&quot;
def gotNoStars = xp.evaluate(expr, doc)

// ===== Si falla, guardar CSV con el orden solicitado y fallar assertion =====
if (gotNoStars != expectedNoStars) {
    def baseDir = FileServer.getFileServer().getBaseDir()
    def formattedDate = ensureFormattedDate()
    def dir = new File(baseDir, &quot;Results/MainTest/${formattedDate}&quot;)
    if (!dir.exists()) dir.mkdirs()
    def file = new File(dir, &quot;Usuarios_fallados.csv&quot;)
    def isNew = !file.exists()

    def userId         = vars.get(&quot;P_Username&quot;)
    def masked         = vars.get(&quot;C_maskedUserName&quot;)
    def requestData    = prev.getSamplerData()
    def responseCode   = prev.getResponseCode()
    def responseMsg    = prev.getResponseMessage()

    // Opcional: limitar tamaño del ResponseData si puede ser enorme
    // def maxLen = 20000
    // def responseDataTrimmed = responseData?.length() &gt; maxLen ? responseData.substring(0, maxLen) + &quot;...[truncated]&quot; : responseData
    def responseDataTrimmed = responseData

    synchronized (this.class) {
        file.parentFile.mkdirs()
        file.withWriterAppend { w -&gt;
            if (isNew) {
                w.writeLine(&quot;P_Username,C_maskedUserName,Got_NoStars,RequestData,ResponseData,responseCode,responsemessage&quot;)
            }
            def line = [
                userId,
                masked,
                gotNoStars,
                requestData,
                responseDataTrimmed,
                responseCode,
                responseMsg
            ].collect { csv(it) }.join(&quot;,&quot;)
            w.writeLine(line)
        }
    }

    throw new AssertionError(&quot;Revisar Usuario. Esperado=&apos;${expectedNoStars}&apos; Obtenido=&apos;${gotNoStars}&apos;&quot;)
}
