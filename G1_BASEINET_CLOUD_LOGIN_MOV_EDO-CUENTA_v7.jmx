            if (isNew) {
                w.writeLine(&quot;P_Username,C_maskedUserName,Got_NoStars,RequestData,ResponseData,responseCode,responsemessage&quot;)
            }
            def line = [
                userId,
                masked,
                gotNoStars,
                requestData,
                responseDataTrimmed,
                responseCode,
                responseMsg
            ].collect { csv(it) }.join(&quot;,&quot;)
            w.writeLine(line)
        }
    }

    throw new AssertionError(&quot;Revisar Usuario. Esperado=&apos;${expectedNoStars}&apos; Obtenido=&apos;${gotNoStars}&apos;&quot;)
