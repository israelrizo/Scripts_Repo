import groovy.json.JsonBuilder;

//String samplerName = sampler.getName();
//log.info(&quot;Current Sampler Name: &quot; + samplerName);

// Retrieve P_Navegations from JMeter User Defined Variables or use default value
if (sampler instanceof org.apache.jmeter.protocol.http.sampler.HTTPSamplerBase) {
    apiUrl = sampler.getUrl().toString();
    def samplerRaw = sampler.getArguments().getArgument(0).getValue();
    def json = new JsonSlurper().parseText(samplerRaw); 
    String navigationData = json.navegations;
    //def requestBody = 
    log.warn(navigationData);
	// Combine values like in signBodyRequest()
	String rawData = apiUrl + navigationData;
	
	// Compute SHA-256 hash
	MessageDigest digest = MessageDigest.getInstance(&quot;SHA-256&quot;);
	byte[] hash = digest.digest(rawData.getBytes(&quot;UTF-8&quot;));
	
	// Encode in Base64 (API might expect this)
	String requestId = Base64.getUrlEncoder().withoutPadding().encodeToString(hash);
	
	// Store in JMeter variable
	vars.put(&quot;P_Request_IDV2&quot;, requestId);
	
	// Log output for debugging
	//log.info(&quot;Generated request_id: &quot; + requestId);
	//log.info(&quot;Used navigation data: &quot; + navigationData);
} else {
    log.warn(&quot;Sampler &apos;&quot; + sampler.getName() + &quot;&apos; does not support getUrl()&quot;);
}
